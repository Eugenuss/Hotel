<Window x:Class="HotelAIS.Windows.AdminMainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d"
        Title="Меню для админа" Height="450" Width="800" WindowState="Maximized" KeyDown="LoginWindow_OnKeyDown">
    <Grid>
        <StackPanel>
            <Line Height="50"/>
            <Button x:Name="RoomsButton" FontSize="28" Content="Комнаты" Click="RoomsButton_Clicked" Margin="276,0"/>
            <Line Height="10"/>
            <Button x:Name="UserButton" FontSize="28" Content="Пользователи" Click="UserButton_Clicked" Margin="276,0"/>
            <Line Height="30"/>
            <Button x:Name="ReturnButton" FontSize="28" Content="Назад" Click="ReturnButton_Clicked" Margin="318,0,321,0"/>
        </StackPanel>
    </Grid>
</Window>
